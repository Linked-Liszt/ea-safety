{
    "experiment": {
        "num_gens": 200,
        "num_runs": 30,
        "log_name": "evo_ac_cart_single",
        "log_path": "checkpoints/evo_ac",
        "env": "CartPole-v1",
        "log_run": false,
        "single":true,
        "print_interval": 1
    },

    
    "evo_ac": {
        "pop_size": 10,
        "value_coeff": 1.0,
        "entropy_coeff": 0.001,
        "recomb_nums": [10],
        "hold_elite":false,
        "learning_rate": [1e-5, 1e-5],
        "mut_scale":  0.0,
        "lr_decay": 0.99

    },

    "neural_net": {
        "learning_rate":3e-3,
        "shared": [
            {
                "type": "Linear",
                "params": [4, 128], 
                "kwargs": {"bias":true}
            }, 
            {
                "type": "ReLU",
                "params": [], 
                "kwargs": {}
            }

        ],
        "policy":
        [
            {
                "type": "Linear",
                "params": [128, 128], 
                "kwargs": {"bias":true}
            },
            {
                "type": "ReLU",
                "params": [], 
                "kwargs": {}
            },
            {
                "type": "Linear",
                "params": [128, 2], 
                "kwargs": {"bias":true}
            }
        ],
        "value": [
            {
                "type": "Linear",
                "params": [128, 1], 
                "kwargs": {"bias":true}
            }
        ]
        
    }
}