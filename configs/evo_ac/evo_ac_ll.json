{
    "evo_ac": {
        "env": "LunarLander-v2",
        "log_path": "output_nets/ant",
        "pop_size": 10,
        "value_coeff": 0.5,
        "entropy_coeff": 0.0,
        "num_gens": 100

    },

    "neural_net": {
        "shared": [
            {
                "type": "Linear",
                "params": [8, 128], 
                "kwargs": {"bias":true}
            }, 
            {
                "type": "ReLU",
                "params": [], 
                "kwargs": {}
            }

        ],
        "policy":
        [
            {
                "type": "Linear",
                "params": [128, 4], 
                "kwargs": {"bias":true}
            }
        ],
        "value": [
            {
                "type": "Linear",
                "params": [128, 1], 
                "kwargs": {"bias":true}
            }
        ]
        
    }
}